{
    "name": "vladislavs/sf-payments",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Migrations\\": "migrations/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes"
    },
    "authors": [
        {
            "name": "Vladislavs Poznaks",
            "email": "vladislavs.poznaks@gmail.com"
        }
    ],
    "require": {
        "nikic/fast-route": "^1.3",
        "doctrine/migrations": "^3.5",
        "doctrine/dbal": "^3.5",
        "vlucas/phpdotenv": "^5.5",
        "ramsey/uuid": "^4.7",
        "nesbot/carbon": "^2.65",
        "doctrine/orm": "^2.14",
        "symfony/cache": "^6.2",
        "ramsey/uuid-doctrine": "^2.0",
        "vlucas/valitron": "^1.4",
        "symfony/console": "^6.2",
        "php-di/php-di": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.13"
    }
}
